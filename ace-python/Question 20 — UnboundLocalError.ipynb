   "source": [
    "# What is an UnboundLocalError?  Write a function\n",
    "# that causes one to occur\n",
    "\n",
    "x = 100   # global variable!\n",
    "\n",
    "def add_to_x():\n",
    "    global x\n",
    "    x = x + 1\n",
    "    \n",
    "    \n",
    "print(f\"Before, x = {x}\")\n",
    "add_to_x()\n",
    "print(f\"After, x = {x}\")"
   ]
